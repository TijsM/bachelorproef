@Article{Malavolta2016,
	author       = {Ivano, Malavolta},
	date         = {2016-10-01},
	title        = {Beyond Native Apps: Web Technologies To The Rescue},
	url          = {https://research.vu.nl/en/publications/beyond-native-apps-web-technologies-to-the-rescue-keynote},
	abstract     = {As of today, mobile software development teams can follow a number of different development and distribution strategies, ranging from native apps, to mobile web apps, hybrid apps, and the recently emerging progressive web apps. This talk provides a state-of-the-art overview of the developmentstrategiesandtechnologiesfordevelopingmobile apps, each of them with its own advantages and drawbacks. In this context, the use of web technologies is discussed as a promising investment for moving forward one of the most intriguing challenges in the world of mobile apps: its fragmentation with respect to mobile platforms. A discussion of research challenges, and thus opportunities, closes the talk.},
